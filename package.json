{
  "name": "ketchup-timer",
  "displayName": "Ketchup Timer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ketchup.startTimer",
        "title": "Ketchup: Start Timer"
      },
      {
        "command": "ketchup.pauseTimer",
        "title": "Ketchup: Pause Timer"
      },
      {
        "command": "ketchup.resumeTimer",
        "title": "Ketchup: Resume Timer"
      },
      {
        "command": "ketchup.stopTimer",
        "title": "Ketchup: Stop Timer"
      },
      {
        "command": "ketchup.startBreak",
        "title": "Ketchup: Break Time"
      },
      {
        "command": "ketchup.startWork",
        "title": "Ketchup: Work Time"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  }
}
